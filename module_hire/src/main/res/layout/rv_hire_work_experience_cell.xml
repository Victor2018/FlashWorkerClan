<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/mClWorkExperience"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="@dimen/dp_21"
    android:paddingRight="@dimen/dp_21"
    android:paddingTop="@dimen/dp_30"
    android:paddingBottom="@dimen/dp_30"
    tools:ignore="ResourceName">

    <TextView
        android:id="@+id/mTvCompany"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/mTvDate"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginEnd="@dimen/dp_10"
        android:textSize="@dimen/dp_28"
        android:textColor="@color/color_333333"
        android:text="九毛九餐饮公司"/>
    <TextView
        android:id="@+id/mTvPosition"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mTvCompany"
        android:layout_marginTop="@dimen/dp_25"
        android:textSize="@dimen/dp_24"
        android:textColor="@color/color_999999"
        android:text="传菜员"/>
    <TextView
        android:id="@+id/mTvDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/mTvCompany"
        app:layout_constraintBottom_toBottomOf="@+id/mTvCompany"
        android:drawablePadding="@dimen/dp_25"
        android:textSize="@dimen/dp_24"
        android:textColor="@color/color_999999"
        android:text="2020.12.12 - 2020.12.26"/>

</androidx.constraintlayout.widget.ConstraintLayout>