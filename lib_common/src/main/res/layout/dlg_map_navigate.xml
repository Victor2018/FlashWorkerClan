<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/dp_27"
    android:background="@drawable/bg_dlg_city_picker"
    tools:ignore="ResourceName">

    <TextView
        android:id="@+id/mTvGaodeMap"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_110"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="center"
        android:textSize="@dimen/dp_32"
        android:textColor="@color/color_333333"
        android:text="高德地图"/>

    <View
        android:id="@+id/line_gaode_map"
        style="@style/base_line_horizontal"
        app:layout_constraintTop_toBottomOf="@+id/mTvGaodeMap"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/mTvBaiduMap"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_110"
        app:layout_constraintTop_toBottomOf="@+id/line_gaode_map"
        android:gravity="center"
        android:textSize="@dimen/dp_32"
        android:textColor="@color/color_333333"
        android:text="百度地图"/>

    <View
        android:id="@+id/line_baidu_map"
        style="@style/base_line_horizontal"
        app:layout_constraintTop_toBottomOf="@+id/mTvBaiduMap"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/mTvTentcentMap"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_110"
        app:layout_constraintTop_toBottomOf="@+id/line_baidu_map"
        android:gravity="center"
        android:textSize="@dimen/dp_32"
        android:textColor="@color/color_333333"
        android:text="腾讯地图"/>

</androidx.constraintlayout.widget.ConstraintLayout>